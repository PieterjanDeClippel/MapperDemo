<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<OutputType>Exe</OutputType>
		<TargetFramework>net10.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="MintPlayer.SourceGenerators.Targets" Version="5.6.0" PrivateAssets="all" IncludeAssets="content" GeneratePathProperty="true" />
	</ItemGroup>

	<!-- We want to use the MapperGenerator -->
	<Import Project="$(PkgMintPlayer_SourceGenerators_Targets)\content\mappergenerator.targets" />
</Project>
